{
    "version": 2,
    "builds": [
      {
        "src": "main.py",
        "use": "@vercel/python"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "main.py"
      }
    ],
    "crons": [
      {
        "path": "/api/sync_notion_to_github",
        "schedule": "0 */1 * * *"
      }
    ]
  }
  